{
    "version":2,
    "builds":[
        {
            "src":"./index.js",
            "use":"@vercel-node"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"/"
        }
    ],
    "env": {
        "MONGODB_URI": "@mongodb-ur"
      },
      "headers": [
        {
          "source": "/(.*)",
          "headers": [
            {
              "key": "Access-Control-Allow-Origin",
              "value": "*"
            }
          ]
        }
      ]
}